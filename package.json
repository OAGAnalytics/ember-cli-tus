{
  "name": "ember-cli-tus",
  "version": "0.0.5",
  "description": "Ember Addon for tus-js-client",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/OAGAnalytics/ember-cli-tus",
  "license": "MIT",
  "author": "Matt Marcum (https://github.com/mattmarcum)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "tusd:pull": "docker pull tusproject/tusd",
    "tusd:start": "docker run -p 1080:1080 -v $(pwd)/tus-uploads:/srv/tusd-data/data tusproject/tusd"
  },
  "dependencies": {
    "ember-auto-import": "^1.3.0",
    "ember-cli-babel": "^7.7.3",
    "ember-decorators-polyfill": "^1.0.4",
    "ember-get-config": "^0.2.4",
    "tus-js-client": "^1.7.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.6.3",
    "babel-eslint": "^10.0.1",
    "broccoli-asset-rev": "^2.7.0",
    "ember-cli": "~3.9.0",
    "ember-cli-addon-docs": "^0.6.10",
    "ember-cli-addon-docs-yuidoc": "^0.2.1",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^1.1.1",
    "ember-cli-deploy-git": "^1.3.3",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-pretender": "^3.1.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-file-upload": "^2.6.1",
    "ember-font-awesome": "^4.0.0-rc.4",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.9.1",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
